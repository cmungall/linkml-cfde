@prefix cfde_phenotype_association_type: <https://w3id.org/linkml/cfde/cfde_phenotype_association_type/> .
@prefix cfde_schema: <https://w3id.org/linkml/cfde/> .
@prefix cfde_subject_ethnicity: <https://w3id.org/linkml/cfde/cfde_subject_ethnicity/> .
@prefix cfde_subject_granularity: <https://w3id.org/linkml/cfde/cfde_subject_granularity/> .
@prefix cfde_subject_race: <https://w3id.org/linkml/cfde/cfde_subject_race/> .
@prefix cfde_subject_role: <https://w3id.org/linkml/cfde/cfde_subject_role/> .
@prefix cfde_subject_sex: <https://w3id.org/linkml/cfde/cfde_subject_sex/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cfde_schema:Biosample a sh:NodeShape ;
    sh:closed true ;
    sh:description "A tissue sample or other physical specimen" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample" ;
    sh:property [ sh:description "An ISO 8601 -- RFC 3339 (subset)-compliant timestamp documenting this biosample's creation time: YYYY-MM-DDTHH:MM:SS±NN:NN" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path cfde_schema:creation_time ],
        [ sh:class cfde_schema:AssayType ;
            sh:description "An OBI CV term ID (from the 'planned process' branch of the vocabulary, excluding the 'assay' subtree) describing the preparation method that produced this biosample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path cfde_schema:assay_type ],
        [ sh:class cfde_schema:IdNamespace ;
            sh:description "A CFDE-cleared identifier representing the top-level data space containing this biosample [part 1 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path cfde_schema:id_namespace ],
        [ sh:description "An identifier representing this biosample, unique within this id_namespace [part 2 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:local_id ],
        [ sh:class cfde_schema:Anatomy ;
            sh:description "An UBERON CV term ID used to locate the origin of this biosample within the physiology of its source or host organism" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path cfde_schema:anatomy ],
        [ sh:description "A persistent, resolvable (not necessarily retrievable) URI or compact ID permanently attached to this biosample" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path cfde_schema:persistent_id ],
        [ sh:description "The id_namespace of the primary project within which this biosample was created [part 1 of 2-component composite foreign key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:project_id_namespace ],
        [ sh:description "The local_id of the primary project within which this biosample was created [part 2 of 2-component composite foreign key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:project_local_id ] ;
    sh:targetClass cfde_schema:Biosample .

cfde_schema:BiosampleDisease a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a C2M2 biosample and a disease positively (e.g. cancer tumor tissue sample) OR negatively (e.g. cancer-free tissue sample) identified for that biosample" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample_disease" ;
    sh:property [ sh:description "The relationship between this biosample and this disease (e.g. 'observed' or '(tested for, but) not observed')" ;
            sh:in ( cfde_phenotype_association_type:0 cfde_phenotype_association_type:1 ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:association_type ],
        [ sh:description "Identifier namespace for this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:biosample_id_namespace ],
        [ sh:class cfde_schema:Disease ;
            sh:description "A Disease Ontology CV term ID describing this disease" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path cfde_schema:disease ],
        [ sh:description "The ID of this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:biosample_local_id ] ;
    sh:targetClass cfde_schema:BiosampleDisease .

cfde_schema:BiosampleFromSubject a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a biosample and its source subject" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample_from_subject" ;
    sh:property [ sh:description "Identifier namespace for this subject" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:subject_id_namespace ],
        [ sh:description "The ID of this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:biosample_local_id ],
        [ sh:description "Identifier namespace for this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:biosample_id_namespace ],
        [ sh:description "The ID of this subject" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:subject_local_id ],
        [ sh:description "The age in years (with a fixed precision of two digits past the decimal point) of this subject when this biosample was taken" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path cfde_schema:age_at_sampling ] ;
    sh:targetClass cfde_schema:BiosampleFromSubject .

cfde_schema:BiosampleGene a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a C2M2 biosample and an Ensembl gene especially relevant to it" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample_gene" ;
    sh:property [ sh:description "The ID of this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:biosample_local_id ],
        [ sh:description "Identifier namespace for this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:biosample_id_namespace ],
        [ sh:class cfde_schema:Gene ;
            sh:description "An Ensembl gene ID" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:gene ] ;
    sh:targetClass cfde_schema:BiosampleGene .

cfde_schema:BiosampleInCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a biosample and a (containing) collection" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample_in_collection" ;
    sh:property [ sh:description "Identifier namespace for this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:biosample_id_namespace ],
        [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:description "The ID of this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:biosample_local_id ] ;
    sh:targetClass cfde_schema:BiosampleInCollection .

cfde_schema:BiosampleSubstance a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a C2M2 biosample and a PubChem substance experimentally associated with that biosample" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "biosample_substance" ;
    sh:property [ sh:description "Identifier namespace for this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:biosample_id_namespace ],
        [ sh:description "The ID of this biosample" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:biosample_local_id ],
        [ sh:class cfde_schema:Substance ;
            sh:description "A PubChem substance ID (SID) describing this substance" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:substance ] ;
    sh:targetClass cfde_schema:BiosampleSubstance .

cfde_schema:Collection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A grouping of C2M2 files, biosamples and/or subjects" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection" ;
    sh:property [ sh:description "Does this collection contain time-series data? (allowed values: [true|false|null] -- true == yes, contains time-series data; false == no, doesn't contain time-series data; null == no info provided)" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path cfde_schema:has_time_series_data ],
        [ sh:description "A very short display label for this collection" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path cfde_schema:abbreviation ;
            sh:pattern "^[a-zA-Z0-9_]+$" ],
        [ sh:description "A human-readable description of this collection" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path cfde_schema:description ],
        [ sh:description "An identifier representing this collection, unique within this id_namespace [part 2 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:local_id ],
        [ sh:class cfde_schema:IdNamespace ;
            sh:description "A CFDE-cleared identifier representing the top-level data space containing this collection [part 1 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path cfde_schema:id_namespace ],
        [ sh:description "A persistent, resolvable (not necessarily retrievable) URI or compact ID permanently attached to this collection" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:persistent_id ],
        [ sh:description "An ISO 8601 -- RFC 3339 (subset)-compliant timestamp documenting this collection's creation time: YYYY-MM-DDTHH:MM:SS±NN:NN" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:creation_time ] ;
    sh:targetClass cfde_schema:Collection .

cfde_schema:CollectionAnatomy a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between an UBERON anatomical term and a C2M2 collection containing experimental resources directly related to the study of the anatomical concept described by that term" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_anatomy" ;
    sh:property [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:class cfde_schema:Anatomy ;
            sh:description "An UBERON term ID" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:anatomy ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ] ;
    sh:targetClass cfde_schema:CollectionAnatomy .

cfde_schema:CollectionCompound a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a compound and a C2M2 collection containing experimental resources directly related to the study of that compound" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_compound" ;
    sh:property [ sh:class cfde_schema:Compound ;
            sh:description "A PubChem or GlyTouCan term ID describing this compound" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:compound ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ] ;
    sh:targetClass cfde_schema:CollectionCompound .

cfde_schema:CollectionDefinedByProject a sh:NodeShape ;
    sh:closed true ;
    sh:description "(Shallow) association between a collection and a project that defined it" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_defined_by_project" ;
    sh:property [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:description "ID of the identifier namespace corresponding to the C2M2 submission containing this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:description "ID of the identifier namespace corresponding to the C2M2 submission containing this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:project_id_namespace ],
        [ sh:description "The ID of this project" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:project_local_id ] ;
    sh:targetClass cfde_schema:CollectionDefinedByProject .

cfde_schema:CollectionDisease a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a disease and a C2M2 collection containing experimental resources directly related to the study of that disease" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_disease" ;
    sh:property [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:class cfde_schema:Disease ;
            sh:description "A Disease Ontology CV term ID describing this disease" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:disease ] ;
    sh:targetClass cfde_schema:CollectionDisease .

cfde_schema:CollectionGene a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a gene and a C2M2 collection containing experimental resources directly related to the study of that gene" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_gene" ;
    sh:property [ sh:class cfde_schema:Gene ;
            sh:description "An Ensembl term ID describing this gene" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:gene ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:description "The ID of this collection" ;
