cfde_schema:Compound a sh:NodeShape ;
    sh:closed true ;
    sh:description "List of (i) GlyTouCan terms or (ii) PubChem 'compound' terms (normalized chemical structures) referenced in this submission; (ii) will include all PubChem 'compound' terms associated with any PubChem 'substance' terms (specific formulations of chemical materials) directly referenced in this submission, in addition to any 'compound' terms directly referenced" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "compound" ;
    sh:property [ sh:description "A human-readable description of this compound" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:description ],
        [ sh:description "A GlyTouCan ID or a PubChem compound ID (CID)" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:id ],
        [ sh:description "A list of synonyms for this compound" ;
            sh:order 2 ;
            sh:path cfde_schema:synonyms ] ;
    sh:targetClass cfde_schema:Compound .

