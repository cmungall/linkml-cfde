cfde_schema:CollectionInCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a containing collection (superset) and a contained collection (subset)" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_in_collection" ;
    sh:property [ sh:description "ID of the identifier namespace corresponding to the C2M2 submission containing the subset collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:subset_collection_id_namespace ],
        [ sh:description "ID of the identifier namespace corresponding to the C2M2 submission containing the superset collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:superset_collection_id_namespace ],
        [ sh:description "The ID of the superset collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:superset_collection_local_id ],
        [ sh:description "The ID of the subset collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:subset_collection_local_id ] ;
    sh:targetClass cfde_schema:CollectionInCollection .

cfde_schema:CollectionPhenotype a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a phenotype and a C2M2 collection containing experimental resources directly related to the study of that phenotype" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_phenotype" ;
    sh:property [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:class cfde_schema:Phenotype ;
            sh:description "A Human Phenotype Ontology CV term ID describing this phenotype" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:phenotype ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ] ;
    sh:targetClass cfde_schema:CollectionPhenotype .

cfde_schema:CollectionProtein a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a protein and a C2M2 collection containing experimental resources directly related to the study of that protein" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_protein" ;
    sh:property [ sh:class cfde_schema:Protein ;
            sh:description "A UniProtKB term ID describing this protein" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:protein ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ] ;
    sh:targetClass cfde_schema:CollectionProtein .

cfde_schema:CollectionSubstance a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a substance and a C2M2 collection containing experimental resources directly related to the study of that substance" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_substance" ;
    sh:property [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:class cfde_schema:Substance ;
            sh:description "A PubChem term ID describing this substance" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:substance ],
        [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ] ;
    sh:targetClass cfde_schema:CollectionSubstance .

cfde_schema:CollectionTaxonomy a sh:NodeShape ;
    sh:closed true ;
    sh:description "Association between a taxon and a C2M2 collection containing experimental resources directly related to the study of that taxon" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "collection_taxonomy" ;
    sh:property [ sh:description "The ID of this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:collection_local_id ],
        [ sh:description "Identifier namespace for this collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:collection_id_namespace ],
        [ sh:class cfde_schema:NcbiTaxonomy ;
            sh:description "An NCBI Taxonomy Database ID identifying this taxon" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path cfde_schema:taxon ] ;
    sh:targetClass cfde_schema:CollectionTaxonomy .

cfde_schema:Dcc a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Common Fund program or data coordinating center (DCC, identified by the given project foreign key) that produced this C2M2 instance" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "DCC" ;
    sh:property [ sh:description "Name of this DCC's primary technical contact" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path cfde_schema:contact_name ],
        [ sh:description "A human-readable description of this DCC" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path cfde_schema:dcc_description ],
        [ sh:description "ID of the identifier namespace for the project record representing the C2M2 submission produced by this DCC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 7 ;
            sh:path cfde_schema:project_id_namespace ],
        [ sh:description "URL of the front page of the website for this DCC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path cfde_schema:dcc_url ],
        [ sh:description "Email address of this DCC's primary technical contact" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path cfde_schema:contact_email ],
        [ sh:description "A short, human-readable, machine-read-friendly label for this DCC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path cfde_schema:dcc_name ],
        [ sh:description "Foreign key identifying the project record representing the C2M2 submission produced by this DCC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 8 ;
            sh:path cfde_schema:project_local_id ],
        [ sh:description "A very short display label for this contact's DCC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path cfde_schema:dcc_abbreviation ;
            sh:pattern "^[a-zA-Z0-9_]+$" ],
        [ sh:description "The identifier for this DCC, issued by the CFDE-CC" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path cfde_schema:id ] ;
    sh:targetClass cfde_schema:Dcc .

cfde_schema:File a sh:NodeShape ;
    sh:closed true ;
    sh:description "A stable digital asset" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "file" ;
    sh:property [ sh:description "If this file is a bundle encoding more than one sub-file, this field gives the local_id of a collection listing the bundle's sub-file contents; null otherwise" ;
            sh:maxCount 1 ;
            sh:order 18 ;
            sh:path cfde_schema:bundle_collection_local_id ],
        [ sh:class cfde_schema:AssayType ;
            sh:description "An OBI CV term ID describing the type of experiment that generated the results summarized by this file" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path cfde_schema:assay_type ],
        [ sh:class cfde_schema:IdNamespace ;
            sh:description "A CFDE-cleared identifier representing the top-level data space containing this file [part 1 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path cfde_schema:id_namespace ],
        [ sh:class cfde_schema:DataType ;
            sh:description "An EDAM CV term ID identifying the type of information stored in this file (e.g. RNA sequence reads): null if is_bundle is set to true" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path cfde_schema:data_type ],
        [ sh:description "(allowed) MD5 checksum for this file [sha256, md5 cannot both be null]" ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path cfde_schema:md5 ],
        [ sh:class cfde_schema:FileFormat ;
            sh:description "An EDAM CV term ID identifying the digital format of this file (e.g. TSV or FASTQ): if this file is compressed, this should be its _uncompressed_ format" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path cfde_schema:file_format ],
        [ sh:description "A persistent, resolvable (not necessarily retrievable) URI or compact ID permanently attached to this file" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path cfde_schema:persistent_id ],
        [ sh:description "A filename with no prepended PATH information" ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path cfde_schema:filename ;
            sh:pattern "^[^/\\:]+$" ],
        [ sh:description "An identifier representing this file, unique within this id_namespace [part 2 of 2-component composite primary key]" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
